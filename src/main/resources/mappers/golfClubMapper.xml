<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.MyGolfTown.golf.mapper.GolfClubMapper">
	<select id="selectGolfClubByUsedClubAndClubType" resultType="com.MyGolfTown.golf.domain.GolfClub">
		SELECT
			`id`
			,`usedClub`
			,`clubType`
			,`clubName`
			,`clubImagePath`
			,`clubSpecification`
			,`clubCount`
			,`clubPrice`
			,`createdAt`
			,`updatedAt`
		FROM
			`golfClub`
		WHERE
			`usedClub`=#{usedClub} AND `clubType` = #{clubType}
	</select>
	
	<select id="selectGolfClub" resultType="com.MyGolfTown.golf.domain.GolfClub">
		SELECT
			`id`
			,`usedClub`
			,`clubType`
			,`clubName`
			,`clubImagePath`
			,`clubSpecification`
			,`clubCount`
			,`clubPrice`
			,`createdAt`
			,`updatedAt`
		FROM
			`golfClub`
	</select>
	
	<select id="selectGolfClubById" resultType="com.MyGolfTown.golf.domain.GolfClub">
		SELECT
			`id`
			,`usedClub`
			,`clubType`
			,`clubName`
			,`clubImagePath`
			,`clubSpecification`
			,`clubCount`
			,`clubPrice`
			,`createdAt`
			,`updatedAt`
		FROM
			`golfClub`
		WHERE
			`id` = #{id}
	</select>
	<select id="selectGolfClubByName" resultType="com.MyGolfTown.golf.domain.GolfClub">
		SELECT
			`id`
			,`usedClub`
			,`clubType`
			,`clubName`
			,`clubImagePath`
			,`clubSpecification`
			,`clubCount`
			,`clubPrice`
			,`createdAt`
			,`updatedAt`
		FROM
			`golfClub`
		WHERE
			`clubName` = #{clubName}
	</select>

	
	
</mapper>